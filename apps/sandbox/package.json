{
  "name": "@penny/sandbox",
  "version": "1.0.0",
  "description": "Secure Python sandbox execution service for PENNY platform",
  "type": "module",
  "main": "./dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "docker:build": "docker build -t penny-sandbox .",
    "docker:run": "docker run --rm -p 3003:3003 penny-sandbox",
    "docker:dev": "docker build -t penny-sandbox-dev . && docker run --rm -p 3003:3003 -v $(pwd):/app penny-sandbox-dev npm run dev",
    "setup": "npm run build && npm run docker:build",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.0",
    "@fastify/helmet": "^11.0.0",
    "@fastify/rate-limit": "^9.0.0",
    "@fastify/type-provider-typebox": "^4.0.0",
    "@sinclair/typebox": "^0.32.0",
    "fastify": "^4.24.0",
    "pino": "^8.15.0",
    "pino-pretty": "^10.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "sandbox",
    "python",
    "code-execution",
    "security",
    "isolation",
    "docker",
    "jupyter",
    "data-science"
  ],
  "author": "PENNY Platform",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/penny-ai/penny",
    "directory": "apps/sandbox"
  },
  "bugs": {
    "url": "https://github.com/penny-ai/penny/issues"
  },
  "homepage": "https://penny.ai",
  "engines": {
    "node": ">=18.0.0",
    "python": ">=3.11.0"
  },
  "os": [
    "linux",
    "darwin"
  ],
  "files": [
    "dist",
    "python",
    "Dockerfile",
    "requirements.txt",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  }
}